    <string name="DialogAskReview_cancel">Später</string>
    <string name="DialogAskReview_content">Teilen Sie Ihre Gedanken zu unserer App. Wenn es Ihnen gefällt, geben Sie uns gerne 5 Sterne!</string>
    <string name="DialogAskReview_ok">Bewerten Sie</string>
