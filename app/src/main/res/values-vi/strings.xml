    <!-- Alert Notifications -->

    <string name="NotificationAlert_IgnoringBatteryOptimizations_title">Cần chú ý!</string>
    <string name="NotificationAlert_IgnoringBatteryOptimizations_text">Mở ứng dụng và cho phép WebDefender luôn hoạt động trong nền.</string>

    <!-- Guide Dialogs -->

    <string name="DialogGuideMain_title">Thiết lập cần thiết</string>
    <string name="DialogGuideMain_content_later">Để đảm bảo chức năng ứng dụng đúng cách, hãy điều chỉnh các thiết lập hệ thống bổ sung.\nBạn có muốn thiết lập ngay bây giờ không?</string>
    <string name="DialogGuideMain_content_hide">Để đảm bảo chức năng ứng dụng đúng cách, hãy điều chỉnh các thiết lập hệ thống bổ sung.\nNếu bạn đã cấu hình tất cả mọi thứ rồi, bạn có thể nhấp vào nút \"Ẩn\".</string>
    <string name="DialogGuideMain_later">Sau đó</string>
    <string name="DialogGuideMain_hide">Ẩn</string>
    <string name="DialogGuideMain_ok">Bắt đầu thiết lập</string>

    <string name="DialogGuideQueryAllPackagesPermission_title">Tính hiển thị ứng dụng</string>
    <string name="DialogGuideQueryAllPackagesPermission_content">Để đảm bảo chức năng ứng dụng phù hợp, việc cấp quyền là cần thiết để đảm bảo khả năng hiển thị của các ứng dụng đã cài đặt.</string>
    <string name="DialogGuideQueryAllPackagesPermission_cancel">Sau đó</string>
    <string name="DialogGuideQueryAllPackagesPermission_ok">Đồng ý</string>

    <string name="DialogGuideIgnoreBatteryOptimizationsPermission_title">Tối ưu hóa pin</string>
    <string name="DialogGuideIgnoreBatteryOptimizationsPermission_content">Để đảm bảo chức năng ứng dụng đúng cách và ngăn ngừa ứng dụng bị đóng, hãy cho phép ứng dụng WebDefender luôn chạy ở nền.</string>
    <string name="DialogGuideIgnoreBatteryOptimizationsPermission_cancel">Sau đó</string>
    <string name="DialogGuideIgnoreBatteryOptimizationsPermission_ok">Đồng ý</string>

    <string name="DialogGuideBootPermission_title">Khởi động tự động</string>
    <string name="DialogGuideBootPermission_content">Để ứng dụng khởi động tự động sau khi thiết bị khởi động và khởi động lại, hãy bật công tắc \"Khởi động tự động\" cho ứng dụng WebDefender.</string>
    <string name="DialogGuideBootPermission_cancel">Sau đó</string>
    <string name="DialogGuideBootPermission_ok">Đồng ý</string>

    <string name="DialogGuideMIUI_title">Bảo vệ tắt máy</string>
    <string name="DialogGuideMIUIPrior12_label_1">Điện thoại của bạn có một tùy chọn bổ sung để ngăn ứng dụng bị đóng trong nền. Việc bật tùy chọn này cho ứng dụng WebDefender là rất quan trọng.</string>
    <string name="DialogGuideMIUIPrior12_label_2">Vui lòng làm theo hướng dẫn để khóa ứng dụng WebDefender trong phần \"Gần đây\".</string>
    <string name="DialogGuideMIUIPrior12_label_3">1. Nhấp vào nút \"Ứng dụng gần đây\".</string>
    <string name="DialogGuideMIUIPrior12_label_4">2. Nhấn và giữ hình ảnh của ứng dụng WebDefender trên màn hình.</string>
    <string name="DialogGuideMIUIPrior12_label_5">3. Nhấn nút \"Khóa\".</string>
    <string name="DialogGuideMIUIPrior12_label_6">Việc khóa ứng dụng sẽ thông báo với hệ thống Android để không chấm dứt quá trình nền của ứng dụng, vì tính năng này rất quan trọng để ứng dụng WebDefender hoạt động đúng cách.</string>
    <string name="DialogGuideMIUI12_label_1">Điện thoại của bạn có một tùy chọn bổ sung để ngăn ứng dụng bị đóng trong nền. Việc bật tùy chọn này cho ứng dụng WebDefender là rất quan trọng.</string>
    <string name="DialogGuideMIUI12_label_2">Vui lòng làm theo hướng dẫn để khóa ứng dụng WebDefender trong ứng dụng \"Bảo mật\".</string>
    <string name="DialogGuideMIUI12_label_3">1. Mở ứng dụng \"Bảo mật\" trên thiết bị của bạn.</string>
    <string name="DialogGuideMIUI12_label_4">2. Nhấn vào biểu tượng bánh răng ở góc trên bên phải.</string>
    <string name="DialogGuideMIUI12_label_5">3. Chọn \"Tăng tốc\" trong phần \"Cài đặt\".</string>
    <string name="DialogGuideMIUI12_label_6">4. Nhấp vào \"Khóa ứng dụng\".</string>
    <string name="DialogGuideMIUI12_label_7">5. Bật công tắc cho ứng dụng WebDefender để khóa nó trong nền.</string>
    <string name="DialogGuideMIUI12_label_8">Việc khóa ứng dụng sẽ thông báo với hệ thống Android để không chấm dứt quá trình nền của ứng dụng, vì tính năng này rất quan trọng để ứng dụng WebDefender hoạt động đúng cách.</string>
    <string name="DialogGuideMIUI_ok">Đồng ý</string>

    <string name="DialogGuideFinish_title">Hoàn tất điều chỉnh cài đặt</string>
    <string name="DialogGuideFinish_content">Bây giờ bạn đã hoàn thành tất cả các cài đặt cần thiết, ứng dụng của bạn sẽ hoạt động mượt mà mà không bị gián đoạn. Nếu bạn cần, bạn có thể quay lại hướng dẫn này trong mục \"Không hoạt động?\" trong menu bên.</string>
    <string name="DialogGuideFinish_ok">Đồng ý</string>

</resources>